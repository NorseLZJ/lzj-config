{
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true
    },
    "vim.easymotion": false,
    "vim.statusBarColorControl": false,
    "vim.hlsearch": true,
    "vim.autoindent": true,
    "vim.useSystemClipboard": true,
    "vim.leader": "\\",
    "vim.handleKeys": {
        "<C-d>": true,
        "<C-f>": true,
        "<C-c>": true,
        "<C-v>": true
    },
    "vim.normalModeKeyBindingsNonRecursive": [{
            "before": ["Z", "Z"],
            "commands": [":q!"]
        },
        {
            "before": ["<leader>", "w"],
            "commands": [":w"]
        },
        {
            "before": ["<leader>", "c"],
            "commands": [":noh"]
        },
        {
            "before": ["<leader>", "s", "p"],
            "commands": [":split"]
        },
        {
            "before": ["<leader>", "v", "p"],
            "commands": [":vsplit"]
        }
    ],
    "vim.insertModeKeyBindings": [{
        "before": ["j", "k"],
        "after": ["<ESC>"]
    }],
    "editor.tabSize": 4,
    "editor.fontSize": 12,
    "editor.renderWhitespace": "none",
    "editor.cursorStyle": "line",
    "editor.formatOnSave": true,
    "editor.fontWeight": "normal",
    "editor.lineNumbers": "on",
    "editor.insertSpaces": true,
    "editor.links": false,
    "editor.wordWrapColumn": 1200,
    "scm.defaultViewMode": "tree",
    "workbench.colorTheme": "Default Dark+",
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.fontWeightBold": "normal",
    "window.zoomLevel": 0,
    "go.formatTool": "gofmt",
    "go.gopath": "/home/lzj/go",
    "go.autocompleteUnimportedPackages": true,
    "go.lintOnSave": "off",
    "go.toolsGopath": "/home/lzj/go/bin",
    "go.goroot": "/usr/local/go",
    "go.gotoSymbol.includeGoroot": true,
    "go.inferGopath": false,
    "go.gotoSymbol.includeImports": true,
    "go.languageServerExperimentalFeatures": {},
    "editor.formatOnPaste": true,
    "terminal.integrated.fontSize": 12,
    "explorer.autoReveal": false,
    "git.autofetch": true,
    "editor.minimap.maxColumn": 1000,
    "explorer.confirmDelete": false,
    "editor.maxTokenizationLineLength": 20000000,
    "debug.onTaskErrors": "debugAnyway",
    "editor.suggestSelection": "first",
    "files.associations": {},
    "workbench.editorAssociations": [],
    "workbench.editor.enablePreview": false,
    "files.defaultLanguage": "cpp",
    "editor.formatOnType": true,
    "editor.snippetSuggestions": "top",
    "workbench.settings.editor": "json",
    "html.format.endWithNewline": true,
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "editor.autoIndent": "keep",
    "editor.highlightActiveIndentGuide": false,
    "workbench.tree.renderIndentGuides": "none",
    "debug.console.wordWrap": false,
    "editor.wordWrap": "wordWrapColumn",
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.autocomplete": "Disabled",
    "[proto3]": {
        "editor.defaultFormatter": "zxh404.vscode-proto3"
    },
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "mblode.pretty-formatter"
    },
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "files.autoGuessEncoding": true,
    "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.cargo.buildScripts.enable": false,
  "rust-analyzer.cargo.autoreload": true,
  "editor.fontSize": 16,
  "rust-analyzer.hover.actions.implementations.enable": false,
  "rust-analyzer.lens.implementations.enable": false,
  "rust-analyzer.hover.actions.run.enable": false,
  "rust-analyzer.lens.run.enable": false,
  "rust-analyzer.hover.actions.debug.enable": false,
  "rust-analyzer.lens.debug.enable": false,
     "better-comments.highlightPlainText": true,
  "better-comments.multilineComments": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": " ",
      "color": "#0aff0b",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#0aff0b",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ]
}
